/*取消弹出框  */
.cancelBox{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.4);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
}
.cancelReason{
  width: 90%;
  background-color: #fff;
  border-radius: 20rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  /* height: 600rpx; */
}
.cancelReason .tops{
 background-color: #f0f0f0;
 line-height: 90rpx;
 font-size: 34rpx;
 text-align: center;
}
.cancelLabel{

  border-bottom: solid 1rpx #f0f0f0;
  margin-left:30rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.choice{
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 20rpx 30rpx;
  font-size: 36rpx;
}
.choice button{
  width: 46%;
}
.choice .give{
 background-color: #fff;
 color: #707070;
 margin-right: 5%;
  border:solid 3rpx #707070;
}

.choice .sure{
 background-color: #ff9900;
 color: #fff;

}

button[disabled]{
 opacity: 0.5;
  background-color: #ff9900 !important;
 color: #fff !important;
}



















