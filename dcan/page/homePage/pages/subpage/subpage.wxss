/*选择排序*/
.allChoice {
  background: #fff;
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 200rpx;
  z-index: 999;
}

.sortBox {
  display: flex;
  font-size: 28rpx;
  margin-top: 18rpx;
  padding: 0rpx 12rpx;
  flex-flow: column;
}
.choice {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 30rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #f8f8f8;
  color: #666;
  width: 750rpx;
}

.choice .choiceW {
  text-align: center;
  border-right: solid 1rpx #f8f8f8;
  width: 27%;
  box-sizing: border-box;
    display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  white-space: nowrap;
}

.term {
  width: 19%;
    display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.icon {
  font-size: 28rpx;
  margin-left: 15rpx;
  transform: rotate(90deg);
}

.term image {
  width: 32rpx;
  height: 32rpx;
}

.choiceBold {
  font-weight: bold;
}


