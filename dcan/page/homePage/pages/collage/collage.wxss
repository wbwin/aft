

.row{
    display: flex;
    flex-direction: row;
    align-items: center;
}
page{
    height:100%;
      background-color: #f8f8f8;
    }
.collageAll{
    background-color: #f8f8f8;
    padding-left: 20rpx;
    padding-right: 20rpx;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}
.title{
    position: relative;
}
.titleIcon{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%
}
.titleText{
    display: flex;
    width: 580rpx;
    background-color: #53beb7;
    margin-left: 30rpx;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    padding-left: 15rpx;
    border-radius: 10rpx;
    color: #fff;
    font-size: 28rpx;
}
.titleDiv{
    position: absolute;
    left: 123rpx;
    top: 40rpx;
    width: 20rpx;
    height: 20rpx;
    background-color: #53beb7;
    transform: rotate(45deg)
}
.shop{
    display: flex;
    flex-direction: column;
    background-color: #fff;
    justify-content: center;
    align-items: center;
    margin-top: 40rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}
.shopName{
    font-weight: bold;
    font-size: 32rpx;
    margin-top: 20rpx;
    color: #666;
}
.shopActive{
    font-size: 24rpx;
    color: #999;
    margin-top: 10rpx;
    margin-bottom: 15rpx;
}
.joinCollage{
