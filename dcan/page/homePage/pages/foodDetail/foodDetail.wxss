page {
  font-size: 30rpx;
}

.grayColor {
  color: #999;
}

.line {
  width: 100%;
  height: 20rpx;
  background-color: #f0f0f0;
}

.lineLast {
  width: 100%;
  height: 130rpx;
  background-color: #f0f0f0;
}

.starBox {
  position: relative;
  display: inline-block;
}

.starsS {
  color: #e8e8e8;
  letter-spacing: 2rpx;
}

.starsY {
  color: #ff8c00;
  position: absolute;
  letter-spacing: 2rpx;
  top: 0;
  left: 0;
  overflow: hidden;
}

.inforName {
  color: #a0a0a0;
  padding: 18rpx 30rpx;
}

.seeMore {
  color: #53beb7;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 24rpx 30rpx;
}

.orange {
  color: #f90;
}

/*内容  */

.imgTopBox {
  height: 340rpx;
  position: relative;
}

.imgTopBox .imageTop {
  width: 100%;
  height: 100%;
}

.shopExplain {
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  width: 90%;
  display: flex;
  flex-direction: column;
  color: #fff;
  padding:2% 5%;
  font-size: 26rpx;
}

.shopExplain text:first-child {
  font-size: 30rpx;
  margin-bottom: 8rpx;
}

.allow {
  display: flex;
  flex-flow: wrap row;
  align-items: center;
  padding: 20rpx 30rpx 0;
  font-size: 28rpx;
}

.allowDetail {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
  width: 49%;
}

.allowDetail.menu {
  color: #53beb7;
}

.allowDetail.sell {
  color: #8a8a8a;
}

.allowDetail image {
  width: 52rpx;
  height: 52rpx;
  margin-right: 10rpx;
}

.choiceMeal {
  display: flex;
  flex-flow: wrap row;
  padding: 20rpx 30rpx 0;
  font-size: 28rpx;
}

.choiceMeal .item {
  color: #888;
  border: solid 3rpx #e0e0e0;
  padding: 14rpx 20rpx;
  border-radius: 8rpx;
  margin-right: 14rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.choiceMeal .item.active {
  color: #53beb7;
  border-color: #53beb7;
  background-color: rgba(6, 193, 174, 0.2);
}

.choiceMeal .item .foodY {
  position: absolute;
  top: 0;
  right: 0;
  width: 30rpx;
  height: 36rpx;
}

.choiceMeal .more {
  width: 100%;
  padding-bottom: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #53beb7;
}

.choiceMeal .more text {
  margin-left: 8rpx;
  font-size: 32rpx;
}

.iconN {
  transform: rotate(90deg);
}

.iconY {
  transform: rotate(-90deg);
}

.foodScord-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  border-bottom: solid 1rpx #f0f0f0;
}

.foodScord-title>text {
  font-size: 28rpx;
}

.foodScord .choice {
  padding: 30rpx 30rpx 10rpx;
  display: flex;
  flex-flow: wrap row;
  font-size: 26rpx;
}

.foodScord .choice .item {
  color: #888;
  border: solid 3rpx #e0e0e0;
  padding: 10rpx;
  border-radius: 8rpx;
  margin-right: 14rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.foodScord .choice .item.active {
  color: #f90;
  border-color: #f90;
}

/*商家信息  */

.inform {
  display: flex;
  flex-direction: column;
}

.informDetail {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 12rpx 30rpx;
  border-top: solid 2rpx #f0f0f0;
  border-bottom: solid 2rpx #f0f0f0;
  font-size: 30rpx;
}

.informDetail .left {
	flex: 6;
  display: flex;
  flex-direction: column;
  color: #a0a0a0;
	overflow: hidden;
}
.call{
	flex: 1;
	border-left: solid 3rpx #e8e8e8;
	text-align: center
}

.informDetail .left .name {
  color: #000;
}

.addressDetail {
  margin: 12rpx 0;
  font-size: 27rpx;
	width: 90%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space:nowrap;
}

.address {
  font-size: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.address image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 5rpx;
}

.address .zj {
  font-size: 24rpx;
  margin-left: 8rpx;
  padding: 8rpx;
  background: #53beb7;
  color: #fff;
  border-radius: 3rpx;
}

.informDetail .right {
  width: 40rpx;
  height: 40rpx;
}

.mealDetail {
  display: flex;
  flex-direction: column;
}

.mealDetail .title {
  color: #484848;
  border-top: solid 1rpx #f0f0f0;
  border-bottom: solid 1rpx #f0f0f0;
  padding: 24rpx 30rpx;
  text-align: center;
}

.mealDetail .title2 {
  color: #484848;
  padding: 24rpx 30rpx;
  text-align: center;
}

.mealKind {
  display: flex;
  flex-direction: row;
  line-height: 70rpx;
  border-bottom: solid 1rpx #f0f0f0;
}

.mealKind .name {
  width: 51%;
  padding-left: 3%;
}

.mealKind .numb {
  width: 28%;
  border-left: solid 1rpx #f0f0f0;
  border-right: solid 1rpx #f0f0f0;
  box-sizing: border-box;
  text-align: center;
}

.mealKind .money {
  width: 18%;
  text-align: center;
}

.mealDetail .mealRemark {
  /* display: flex;
  flex-direction: row;
  align-items: center;
  border-top: solid 1rpx #f0f0f0;
  border-bottom: solid 1rpx #f0f0f0; */
  padding: 24rpx 30rpx;
  font-size: 28rpx;
}

.disc {
  width: 9rpx;
  height: 9rpx;
  border-radius: 50%;
  background-color: #333;
  margin-right: 16rpx;
  display: inline-block;
}

/*购买须知  */

.payNotice {
  display: flex;
  flex-direction: column;
}

.payNotice .word {
  display: flex;
  flex-direction: column;
  border-top: solid 1rpx #f0f0f0;
  padding: 16rpx 30rpx;
  color: #484848;
  line-height: 50rpx;
  font-size: 28rpx;
}

.payNotice .word .disc {
  background-color: #484848;
  margin-top: 20rpx;
}

.payNotice .wordName {
  color: #f90;
}

.wordRule {
  display: flex;
  flex-direction: row;
}

/*评价*/

.userpj {
  /* margin-top:16rpx; */
  border-bottom: 1rpx solid #dedede;
  display: flex;
  flex-direction: row;
  padding: 3% 30rpx;
  position: relative;
}

.topImg {
  padding: 10rpx 0 0 0;
  width: 32rpx;
  height: 32rpx;
}

.userpjHead image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.userpjContent {
  display: flex;
  flex-direction: column;
  flex: 2;

}

.username {
  margin-bottom: 8rpx;
  margin-top: 10rpx;
}

.plstars {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex: 2;
}

.star {
  color: #ffe628;
}

.timePj {
  font-size: 28rpx;
  color: #888;
  margin-left: 12rpx;
}

.content {
  margin: 5px 0;
}

.content text {
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break:break-all;

}

.imgPj {
  display: flex;
  flex-flow: row wrap;
  margin-top: 10rpx;
}

.plimg {
  margin-right: 10rpx;
  margin-bottom: 15rpx;
  width: 140rpx;
  height: 140rpx;
}

.allpj {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 16rpx 0;
}

.infroMore {
  padding: 30rpx;
  border-bottom: solid 30rpx #eee;
}

.infroMore .title {
  margin-bottom: 20rpx;
}

.infroMore .detail {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.infroMore .detail .word {
  display: flex;
  flex-direction: column;
  line-height: 42rpx;
  font-size: 28rpx;
}

/*相关推荐  */

.elect {
  padding: 30rpx;
}

.elect .title {
  color: #a0a0a0;
}

.electShop {
  display: flex;
  flex-direction: row;
  border-bottom: solid 1rpx #eee;
  padding: 18rpx 0;
}

.electShop image {
  flex: none;
  width: 24%;
  height: 140rpx;
  margin-right: 12rpx;
}

.electNumb {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.electInfor {
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  justify-content: space-between;
  flex: auto;
}

.electShop .name {
  color: #000;
  font-size: 30rpx;
}

.electShop .times {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 26rpx;
}

.moneyColor {
  font-size: 30rpx;
  color: #53beb7;
  padding-right: 8rpx;
}

.elect .more {
  width: 100%;
  padding-top: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.elect .more text {
  margin-left: 8rpx;
  font-size: 32rpx;
  color: #8a8a8a;
  font-weight: 700;
}

/*还看了 更多商家 */

.advert {
  padding: 30rpx;
}

.advert .title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: 16rpx;
}

.advert .title text:last-child {
  color: #e0e0e0;
}

.shops {
  border-top: solid 1rpx #eee;
  display: flex;
  flex-direction: row;
  padding: 16rpx 0;
}

.shops image {
  width: 28%;
  height: 170rpx;
  border-radius: 4rpx;
  margin-right: 12rpx;
}

.shopDetail {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 8rpx 0;
  flex: auto;
  font-size: 28rpx;
}

.shopDetail .name {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.shopDetail .name text:first-child {
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 80%;
}

.shopDetail .name text:last-child {
  font-size: 26rpx;
  color: #999;
}

.shopDetail .shopAdrs {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 26rpx;
}

.tipsBox {
  color: #ff8c00;
  border: solid 1rpx #ff8c00;
  font-size: 24rpx;
  border-radius: 2rpx;
  padding: 2rpx;
  margin-right: 2rpx;
}

.shopAdrs .money {
  color: #53beb7;
  font-size: 36rpx;
}

.shopAdrs .money .marks {
  color: #999;
  text-decoration: line-through;
  font-size: 26rpx;
  padding-left: 8rpx;
}

/*底部购买  */

.pay {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100rpx;
  border-top: solid 1rpx #eee;
}

.pay .money {
  font-size: 50rpx;
  color: #53beb7;
  margin-left: 20rpx;
}

.pay .market {
  margin-top: 12rpx;
  font-size: 26rpx;
  margin-left: 16rpx;
}

.pay .money text {
  font-size: 30rpx;
}

.pay .payBut {
  background-color: #f90;
  color: #fff;
  position: absolute;
  right: 0;
  bottom: 0;
  height: 100%;
  border-radius: 0;
  font-size: 32rpx;
  line-height: 100rpx;
}

/*图片弹出框  */

.enlargeBox {
  background-color: #585858;
}

.closeImg {
  position: absolute;
  background-color: #282828;
  top: 12rpx;
  left: 12rpx;
  color: #fff;
  font-size: 50rpx;
}




