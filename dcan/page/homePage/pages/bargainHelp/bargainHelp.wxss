.bargainButton:active{
  box-shadow: inset 2px 2px 4px #333;
}
/*砍价提示手气弹框  */

.modelBack {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 1000;
}

.model {
  background-color: #fff;
  border-radius: 10rpx;
  width: 80%;
  display: flex;
  flex-direction: column;
  text-align: center;
}

.model-title {
  font-size: 34rpx;
  font-weight: 700;
  color: #ffcc01;
  padding: 30rpx 0;
}

.model-txt {
  color: #6a6a6a;
  font-size: 30rpx;
  margin-bottom: 30rpx;
}

.model-but {
  flex: 1;
  font-size: 30rpx;
  color: #53beb7;
  border-top: solid 1px #dedede;
  padding: 14rpx 0;
}
.bargainButtonSure{
   /* box-shadow: inset 0px 2px 4px #333; */
   background-color: #999
}
/* 砍价进度条 */
.wordLR-degree {
  width: 95%;
  margin: 30rpx auto 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.wordLR-degree .price {
  width: 90%;
  margin-bottom: 10rpx;
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.degreeBox {
  background-color: #c0c0c0;
  position: relative;
  width: 80%;
  height: 10rpx;
  margin-top: 16rpx;
}

.degreeL, .degreeR, .degreeC {
  position: absolute;
  top: -10rpx;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  z-index: 3;
}

.circle {
  width: 50%;
  height: 50%;
  background-color: #fff;
  border-radius: 50%;
}

.degreeL {
  background-color: #53beb7;
  left: -15rpx;
}

.degreeR {
  right: -15rpx;
  background-color: #c0c0c0;
}

.finish {
  position: absolute;
  background-color: #53beb7;
  top: 0;
  left: 0;
  height: 100%;
  z-index: 2;
  transition: all 0.5s linear;
}

.degreeC {
  right: -15rpx;
  background-color: #ee7942;
}

.degreeC.active {
  z-index: 10;
}

.result {
  font-size: 26rpx;
  position: absolute;
  border-radius: 12rpx;
  padding: 8rpx 12rpx;
  border: solid 1px #dedede;
  text-align: center;
  top: 30rpx;
  left: 100%;
  transform: translateX(-50%);
  color: #ee7942;
  background-color: #fff;
}

.result .arrow {
  width: 16rpx;
  height: 16rpx;
  box-sizing: border-box;
  border-top: solid 1px #dedede;
  border-left: solid 1px #dedede;
  transform: rotate(45deg);
  background: #fff;
  position: absolute;
  top: -8rpx;
  left: 50%;
  margin-left: -8rpx;
}
.noBargain{
  text-align: center;
  padding-top:50%; 
  font-size: 28rpx;
  z-index: 99999;
  
}
.noBargain image{
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 5%;
  z-index: 99999;
}