}
/*点餐人  */
.goShare{
  margin-bottom: 50rpx;
}
.people-box,.content{
  display: flex;
  flex-direction: column;
  background-color: #fff;
}
.content{
  padding-left: 12rpx;
}
.people{
border-bottom: 1rpx solid #eee;
padding: 30rpx 20rpx;
display: flex;
flex-direction: column;
}
.people .title{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between
}
.people .title .name{
 font-size: 34rpx;
 flex: auto;
overflow: hidden;
display: flex;
flex-flow: nowrap row;
}
.name-txt{
 overflow: hidden;
 text-overflow: ellipsis;
 white-space: nowrap;
}
 .name-image{
 width: 34rpx;
 height: 34rpx;
 margin-left: 5rpx;
 flex: none;
}
.people-numb{

  border-radius: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  width: 50rpx;
  height: 50rpx;
  font-size: 34rpx;
  margin-right: 12rpx;
  flex: none;
} 
.cost{
  background-color: #FF7F24;
  font-weight: 700;
}


.people .title .but{
 flex: none;
 display: flex;
 flex-direction: row;
 align-items: center;
 font-size: 26rpx;
}
.but-revise,.but-close{
 border-radius: 4rpx;
 padding:6rpx 16rpx; 
 margin-left: 16rpx;
text-align: center;
}
.but-revise{
  color: #53beb7;
   border: solid 1rpx #53beb7;
}
.but-close{
   color: #a0a0a0;
   border: solid 1rpx #a0a0a0;
}

.foods-box{
margin-left: 70rpx;
margin-top: 26rpx;
color: #a0a0a0;
display: flex;
flex-direction: column;
font-size: 28rpx;
flex: auto;
}
.foods{
  display: flex;
flex-direction: row;
margin-bottom: 16rpx;
}
.foodName{
  width: 70%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.foodNumb{
  width: 10%;
}
.foodPrice{
  width:20%;
  text-align: right;
  font-size: 30rpx;
  color: #909090;
}


.sum{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  color: #a0a0a0;
  font-size: 30rpx;
  padding: 30rpx 20rpx;
  align-items: baseline
}
.totalPrice{
 font-size: 34rpx;
 color: #666;
 margin-left: 20rpx;
}


.body-me-goodss
{
  border:1rpx solid #808080;
color:#808080;
font-size: 26rpx;
margin-left:20rpx;
width: 110rpx;
text-align: center;
height: 40rpx;
line-height: 40rpx
}
.nowraptxt{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 60%;
}

/*点餐人颜色  */

 .people-box .people:nth-child(1n+0) .people-numb {
    background-color: #FF4040;
}
 .people-box .people:nth-child(2n+0) .people-numb  {
    background-color: #ff6000;
}

.people-box .people:nth-child(3n+0) .people-numb {
    background-color: #ffb038;
}

 .people-box .people:nth-child(4n+0) .people-numb  {
    background-color: #72d53b;
}

.people-box .people:nth-child(5n+0) .people-numb  {
    background-color: #6dd3dc;
}

.people-box .people:nth-child(6n+0) .people-numb  {
    background-color: #3199e8;
}

.people-box .people:nth-child(7n+0) .people-numb  {
    background-color: #bc97ff;
}

