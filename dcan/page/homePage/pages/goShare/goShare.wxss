/*新版  */
page{
  background-color: #E0E0E0;
}
.goShareTop{
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-color: #53beb7;
  color: #fff;
  align-items: center;
}
.shopIcon{
  width: 160rpx;
  height: 160rpx;
  margin:40rpx 0 30rpx;
  border-radius: 50%;
}
.shopName{
  font-size: 36rpx;
  font-weight: 700;
}
.shopPeople{
  color: #E8E8E8;
  font-size: 28rpx;
  margin-top: 20rpx;
  width: 96%;
  overflow: hidden;
  display: flex;
  flex-flow: nowrap row;
