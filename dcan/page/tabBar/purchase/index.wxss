/*tab top*/
.top {
  position: fixed;
  top: 0;
  z-index: 2;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 16px;
  background: #fff;
}
.noorders{
  text-align: center;
  padding-top:30%; 
  font-size: 28rpx;
  z-index: 99999;
}
.noorders image{
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 5%;
  z-index: 99999;
}
.tab01, .tab02, .tab03, .tab04{
    font-size: 26rpx;
    width: 25%;
    display: block;
    flex-wrap: wrap;
    text-align: center;
}
.waiterNum{
    border-radius:25px;
    display: flex;
    border: solid 1px #9FB6CD;
    width:320rpx;
    text-align: center;
}

.top .active {
  color: #900;
  border-bottom: solid 2px #900;
}

.show {
  display: block !important;
}

.tab-bd {
  padding: 80rpx 0rpx 0rpx 0;
  height: 100%;
}

.tab-bd01, .tab-bd02, .tab-bd03,.tab-bd04 {
  display: none;
  height: 100%;
}
/*没内容*/
.not {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #fff; 
}
.iicon {
    width: 48rpx;
    height: 48rpx;
    padding: 60rpx;
}
.zeroindent {
    font-size: 28rpx;
}
.stroll {
    font-size: 28rpx;
    color: red;
