/*加载动画  */
.more{
	padding: 1%;
	text-align: center;
	font-size: 28rpx;
	color: #666;
    background-color: #fff;
}

.more image{
	width: 100rpx;
	height: 72rpx;
}
.with-line {
    font-size: 26rpx;
    color: #999;
    position: relative;
    text-align: center;
     padding: 20rpx 0;
}

.with-line:before {
    content: "";
    border-top: 1px solid #ededed;
    display: block;
    position: absolute;
    width: 200rpx;
    top: 35rpx;
    left: 0
}
.with-line:after {
    content: "";
    border-top: 1px solid #ededed;
    display: block;
    position: absolute;
    width: 200rpx;
    top: 35rpx;
    right: 0
}