/*优惠券弹出框*/

.conpouback {
    background: #000;
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 888;
    left: 0;
    top: 0;
    opacity: 0.4;
}

.boxwbk {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 888;
    left: 0;
    top: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 32rpx;
}

.conpouBox {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 80%;
    position: relative;
    background: -webkit-linear-gradient(top, #fe6147, #fa394b);
}

.conpouBox-top {
    width: 100%;
    height: 300rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
