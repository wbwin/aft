page {
    background-color: #f8f8f8;
    font-size: 28rpx;
}

.shop {
    padding: 0 3%;
    overflow: hidden;
}

.shops {
    padding: 3% 0;
    border-bottom: 1rpx solid #dedede;
    display: flex;
}

.shopImg {
    width: 160rpx;
    flex: none;
    display: flex;
    flex-direction: column;
}

.topImg {
    position: relative;
}

.closeOpen {
    flex: 2;
}

.img {
    width: 160rpx;
    height: 160rpx;
    margin-right: 28rpx;
    flex-flow: row;
    flex: none;
}

.daV {
    width: 32rpx;
    height: 32rpx;
    border: 5rpx solid #fff;
    border-radius: 50%;
    position: absolute;
    bottom: 0rpx;
    right: -5rpx;
}

.detail {
    display: flex;
    flex-direction: column;
    flex: auto;
    margin-left: 20rpx;
}

.shopname {
    display: flex;
    align-items: center;
    font-weight: bold;
    color: #333;
}

.send-distance {
    display: flex;
    flex-direction: row;
    /*justify-content: space-between;*/
    font-size: 24rpx;
    line-height: 40rpx;
}

.send {
    display: inline-block;
    color: #888;
    text-align: left;
    flex: none;
}

.distance {
    display: inline-block;
    color: #888;
    text-align: right;
    flex: auto;
}

.acti {
    display: flex;
    border-top: 1rpx solid #dedede;
}

.x {
    font-size: 26rpx;
    color: #d9d9d9;
    flex: auto;
    text-align: right;
    padding: 2% 2% 0 0;
}

.activity {
    padding: 8rpx 8rpx 0 8rpx;
    display: block;
    font-size: 24rpx;
    line-height: 40rpx;
    color: #888;
}

.activity-more {
    position: absolute;
    top: 6rpx;
    right: 10rpx;
    font-size: 42rpx;
}

.activity .moreb {
    transform: rotate(90deg);
}

.activity .moret {
    transform: rotate(270deg);
}

.bl {
    /*display: block;*/
    margin-bottom: 2px;
    white-space: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.bl-jian {
    color: #fff;
    background: #ff4040;
    padding: 5rpx;
    margin-right: 15rpx;
}

.bl-tuan {
    color: #fff;
    background: #1988f9;
    padding: 5rpx;
    margin-right: 15rpx;
}

.bl-kan {
    color: #fff;
    background: yellowgreen;
    padding: 5rpx;
    margin-right: 15rpx;
}

.bl-miao {
    color: #fff;
    background: green;
    padding: 5rpx;
    margin-right: 15rpx;
}

.bl-zhe {
    color: #fff;
    padding: 5rpx;
    margin-right: 15rpx;
    background: #f4a460;
}

.bl-xin {
    color: #fff;
    background: #53beb7;
    padding: 5rpx;
    margin-right: 15rpx;
}

.bl-xian {
    color: #fff;
    background: #ebd500;
    padding: 5rpx;
    margin-right: 15rpx;
}

.bl-lin {
    color: #fff;
    background: #aa135f;
    padding: 5rpx;
    margin-right: 15rpx;
}

.nothing {
    text-align: center;
    padding: 10% 3%;
    font-size: 28rpx;
}

/*选择排序*/

.choice {
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 30rpx;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #dedede;
    color: #666;
}

.choice .choiceW {
    text-align: center;
    border-right: solid 1rpx #dedede;
    width: 27%;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.term {
    width: 19%;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.icon {
    font-size: 28rpx;
    margin-left: 15rpx;
    transform: rotate(90deg);
}

.term image {
    width: 32rpx;
    height: 32rpx;
}

.choiceBold {
    font-weight: bold;
}

/*搜索页面*/

.searchHistory, .searchHot {
    padding: 3%;
}

.history-top {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    font-size: 30rpx;
    border-bottom: solid 2rpx #dedede;
    padding: 0rpx 10rpx 8rpx 10rpx;
}

.historyimg {
    display: flex;
    flex-direction: row;
}

.historyimg image {
    width: 48rpx;
    height: 48rpx;
}

.imgschu {
    margin-right: 20rpx;
    padding: 0 18rpx;
}

.imgBack {
    padding: 0 30rpx;
}

.history-detail {
    display: flex;
    flex-flow: wrap row;
    margin-top: 12rpx;
}

.history-detail .word {
    font-size: 30rpx;
    padding: 10rpx;
    border: solid 1rpx #dedede;
    border-radius: 10rpx;
    margin-bottom: 12rpx;
    margin-right: 12rpx;
}

.hot-top {
    display: block;
    font-size: 30rpx;
    border-bottom: solid 2rpx #dedede;
    padding: 0rpx 10rpx 10rpx 10rpx;
}

/*弹出排序选择框*/

.choiceBack {
    background: #f8f8f8;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-end;
}

.choiceBack-cloce {
    color: #999;
    margin-bottom: 30rpx;
    font-size: 50rpx;
    font-weight: 800;
    padding: 12rpx;
}

.allChoice {
    background: #fff;
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 999;
}

.sortBox {
    display: flex;
    flex-flow: wrap row;
    font-size: 28rpx;
    margin-top: 18rpx;
    padding: 0rpx 12rpx;
}

.feature-sort {
    display: flex;
    flex-flow: wrap row;
    justify-content: space-between;
    margin: 20rpx;
}

.sortChoice {
    padding: 16rpx;
    border: solid 1rpx #dedede;
    border-radius: 10rpx;
    text-align: center;
    width: 25%;
    margin-left: 20rpx;
    margin-bottom: 20rpx;
}

.sortChoice:hover {
    border: solid 1rpx #f08080;
}

.sortbox {
    display: flex;
    flex-flow: column;
    font-size: 28rpx;
    /*padding: 0rpx 12rpx;*/
}

.termbox {
    display: flex;
    flex-flow: column;
    font-size: 28rpx;
    /*padding: 0rpx 12rpx;*/
}

.termFeature {
    display: flex;
    flex-direction: column;
}

.featureTop {
    background: #f8f8f8;
    padding: 10rpx 12rpx;
}

.feature-content {
    display: flex;
    flex-flow: wrap row;
    margin-top: 20rpx;
}

.feature-content .text {
    padding: 16rpx;
    border: solid 1rpx #dedede;
    border-radius: 10rpx;
    text-align: center;
    width: 16%;
    margin-left: 20rpx;
    margin-bottom: 20rpx;
}

.feature-content .textred {
    border: solid 1rpx #53beb7;
    color: #53beb7;
}

.termActivity {
    display: flex;
    flex-direction: column;
}

.activity-content {
    /* display: flex;
  flex-direction: column; */
    float: left;
}

.choiceDetail {
    /* display: flex;
  flex-direction: row;
  align-items: center; */
    padding: 25rpx;
    font-size: 28rpx;
    width: 42%;
    float: left;
}

.choiceColor {
    color: #53beb7;
}

.activityTop {
    padding: 6rpx;
    color: #fff;
    margin-right: 12rpx;
    font-size: 26rpx;
}

.activityName {
    margin-right: 26rpx;
}

.termboxBot {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding-left: 25rpx;
}

.termboxBot .but {
    padding: 25rpx 56rpx;
    background: #53beb7;
    color: #fff;
}

/* 推荐商品 */

.recommendBox {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 888;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.recommend {
    width: 90%;
    height: 77%;
    background: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 6rpx;
}

.recommend_title {
    width: 100%;
    font-size: 32rpx;
    font-weight: 700;
    color: #fff;
    height: 12%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #53beb7;
}

.recommend_content {
    width: 100%;
    font-size: 30rpx;
    background: #f8f8f8;
    height: 74%;
    padding-bottom: 10rpx;
}

.recommend_food {
    display: flex;
    flex-direction: row;
    background: #fff;
    margin-top: 20rpx;
    width: 96%;
    margin-left: 2%;
    overflow: hidden;
    border-radius: 10rpx;
}

.recommend_food image {
    width: 25%;
    margin: 12rpx;
    flex: none;
}

.recommend_word {
    display: flex;
    flex-direction: column;
    padding: 16rpx 0rpx;
    justify-content: space-between;
    flex: auto;
    position: relative;
}

.recommend_word .price {
    color: #ee2c2c;
    margin-bottom: 16rpx;
}

.goShop {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100px;
    height: 90px;
    flex: none;
    border-left: dashed 3rpx #dedede;
    background: red;
    color: #fff;
    font-weight: 600;
}

.recommend_close {
    width: 100%;
    flex: auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.recommend_close button {
    width: 90%;
}

.cat {
    text-align: center;
}

.cat1 {
    width: 20%;
    margin: 2%;
    display: inline-block;
    font-size: 26rpx;
    /* float: left; */
}

.cat1 image {
    width: 80rpx;
    height: 80rpx;
}

.cat2 {
    padding: 10rpx 0;
}

