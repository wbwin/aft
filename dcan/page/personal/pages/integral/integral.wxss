.card-content{
  display: flex;
  flex-direction: column;
  padding: 30rpx 0rpx 0rpx 20rpx;
}

.card-price{
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  justify-content: center;
  margin: 50rpx 0rpx;
}
.card-price .word{
  font-size: 35rpx;
  margin-right: 10rpx;
  margin-bottom: 16rpx;
}
.card-price .symbol{
   font-size: 50rpx;
  margin-right: 20rpx;
    margin-bottom: 16rpx;
}
.card-price .price{
  font-size: 120rpx;
}
.btn{
  font-size: 50rpx;
