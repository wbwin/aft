.card{
  margin-top: 5rpx;
  width: 100%;
  background:-webkit-linear-gradient( left,#1E90FF,red);
  color: #fff;
  border-radius: 30rpx;
  overflow: hidden;
  position: relative;
}



.myinvoice{
  margin-top: 5%;
  background-color:#fff;
  box-shadow: 0 0 5px #999;
  display: flex;
  border-radius: 10rpx;
  height: 160rpx;
  padding: 20rpx 0;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.name{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex: 4;
  color: #53beb7;
  font-size: 34rpx;
}


.edit{
   border-left: 1rpx solid #dedede;
  flex: 2;

