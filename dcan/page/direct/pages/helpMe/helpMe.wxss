page{
	background-color: #f8f8f8;
}
.ad image{
	width: 100%;
	height: 320rpx;
}
.float{
	margin: 3%;
	position: absolute;
	width: 94%;
	top: 4%;
	left: 0;
	text-align: center;
	color: #fff;
}
.comeOn{
	background:linear-gradient(to right top,#53beb7,#a1dbd7,Azure);
	padding-bottom:5%;	
}
.logo{
	position: relative;
	top: -50rpx;
	margin: 0 auto;
	background-color: #fff;
	width: 100rpx;
	height: 100rpx;
	border: 1rpx solid #dedede;
	border-radius: 50%;
}
.logo image{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
}
.title{
	font-size: 36rpx; 
	font-weight: bold;
}

.jy{
	display: flex;
	justify-content: center;
	margin: 10% 0;	
}
.jiayou{
	width: 250rpx;
	height: 250rpx;
	background-color: #53beb7;
	border-radius: 50%;
	border: 1rpx solid #fff;
}

.jiayou text{
	font-size: 34rpx; 
	font-weight: bold;
	line-height: 250rpx;
}

.userImg{
	display: flex;
	justify-content: center;
	margin-bottom: 2%;
}
.userImg image{
	width: 45rpx;
	height: 45rpx;
	border: 1rpx solid #fff;
	border-radius: 50%;
}

.shop{
	background-color: #fff;
	color: #666;
	padding: 5% 3%;
	display: flex;
	align-items: center;
	font-size: 28rpx;
	text-align: left;
}
.L{
	flex: 1.5;
}
.L image{
	width: 80rpx;
	height: 80rpx;
	border-radius: 50%;
	border: 1rpx solid #dedede;
}
.C{
	flex: 7;
}
.R{
	flex: 2;
	text-align: center;	
	font-size: 30rpx;
}
.R text{
	padding: 4% 16%;
	border: 1rpx solid #53beb7;
	border-radius: 10%;
	color: #53beb7;
}

.more{
	padding: 5% 0 20% 0;
	color: #666;
	display: flex;
	justify-content: center;
}

.more view{
	font-size: 32rpx;
	width:300rpx;
	height: 70rpx;
	background-color: #fff;
	text-align: center;
	line-height: 70rpx;
	color: #53beb7;
	border: 1rpx solid #53beb7;
	border-radius: 80rpx;
}

.fix{
	position: fixed;
	width: 94%;
	height: 70rpx;
	bottom: 0;
	left: 0;
	padding: 3%;
	display: flex;
	background-color: #fff;
	font-size: 34rpx;
	align-items: center;
}
.share{
	flex:1;
	background-color: #53beb7;
	color: #fff;
	font-size: 34rpx;
	margin-left: 2%;
}
.shen{
	flex:1;
	background-color: #fff;
	border: 1rpx solid #53beb7;
	color: #53beb7;
	font-size: 34rpx;
	margin-right: 2%;
}

.dagou{
   width:230rpx;
   height:245rpx;
}

.shareImg{
   width:100%;
   height:100%;
   background:#dedede;
   opacity:0.5;
   z-index:888;
   position:fixed;
   left:0px;
   top:0px;
}
.simg{
   position:fixed;
   width: 80%;
   left:10%;
   bottom:17%;
   z-index:999;
}

.cancel{
    position:fixed;
    width: 80%;
    left:10%;
    bottom:10%;
    z-index:999;
	text-align: center;
}
.cancel image{
	width: 40rpx;
	height: 40rpx;
}

/*********************** 红包  *********************************/

.redBox-tc {
    background-color: rgba(0, 0, 0, 0.6);
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    z-index: 999;
}


.red-tearBox {
    /* background-color: #f20e46; */
    /* background-color: #fb5a57; */
    width: 82%;
    color: #fcea59;
    overflow: hidden;
    border-radius: 18rpx;
    margin-top: -120rpx;
}


.red-getBox {
    width: 85%;
    position: relative;
    margin-top: -90rpx;
}

.red-getBox image {
    width: 100%;
    height: 800rpx;
}

.red-tearBox image {
    width: 100%;
    height: 300rpx;
    margin-bottom: 20rpx;
}

.getBox-price {
    position: absolute;
    top: 22%;
    left: 50%;
    transform: translateX(-50%);
    letter-spacing: 2rpx;
    font-weight: 700;
    font-size: 34rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.getBox-price .red-txt {
    color: #fbd345;
    margin-bottom: 3rpx;
}

.getBox-price .red-numb {
    color: #ff565d;
}

.getBox-price .red-numb text {
    font-size: 60rpx;
}

.getBox-tips {
    color: #f8f8f8;
    position: absolute;
    bottom: 5%;
    left: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    height: 60rpx;
    width: 68%;
    margin-left: 16%;
    font-size: 26rpx;
}

.getBox-tips .tipsLine {
    flex: 1;
    height: 1px;
    background-color: #f8f8f8;
}

.getBox-tips .tipsTxt {
    margin: 0 12rpx;
}


.closeRed {
    position: absolute;
    bottom: 13%;
    left: 50%;
    transform: translateX(-60%);
    width: 70rpx;
    height: 70rpx;
    border: solid 1px #fff;
    color: #fff;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    overflow: hidden;
    font-size: 38rpx;
    border-radius: 50%;
}