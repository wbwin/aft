/*点赞 */

.dzanBack {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
}

.zanBox {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  pointer-events: none;
  z-index: 9;
}

.zanBox image {
  width: 160rpx;
  height: 160rpx;
}

.cartoonZan image {
  animation: zan 1000ms 1;
  animation-timing-function: linear;
}

@keyframes zan {
  0% {
    opacity: 0;
    transform: scale(1, 1);
  }

  40% {
    opacity: 1;
    transform: scale(2.3, 2.3);
  }

  44% {
    transform: rotate(10deg) scale(2.3, 2.3);
  }

  48% {
    transform: rotate(0deg) scale(2.3, 2.3);
  }

  52% {
    transform: rotate(-10deg) scale(2.3, 2.3);
  }

  56% {
    transform: rotate(10deg) scale(2.3, 2.3);
  }

  60% {
    transform: rotate(0deg) scale(2.3, 2.3);
  }

  64% {
    transform: rotate(-10deg) scale(2.3, 2.3);
  }

  68% {
    transform: rotate(10deg) scale(2.3, 2.3);
  }

  72% {
    transform: rotate(0deg) scale(2.3, 2.3);
  }

  76% {
    transform: rotate(-10deg) scale(2.3, 2.3);
  }

  100% {
    opacity: 0;
    transform: scale(1, 1);
  }
}

